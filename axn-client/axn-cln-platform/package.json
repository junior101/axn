{
  "name": "axnikitenko.platform",
  "version": "0.1.0",
  "description": "Personal Website",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "author": "Aleksey Nikitenko <axnikitenko@gmail.com>",
  "license": "MIT",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "rimraf": "rimraf",
    "clean": "npm cache clean && npm run rimraf -- build",
    "build:development": "npm run clean && webpack --config config/webpack.development.js --progress --profile",
    "build:production": "npm run clean && webpack --config config/webpack.production.js  --progress --profile --bail",
    "start:development": "npm run clean && webpack-dev-server --config config/webpack.development.js --progress --profile --watch --content-base src/",
    "start:production": "npm run clean && npm run build:production && http-server -p 3000 build --cors"
  },
  "dependencies": {
    "@angular/common": "~2.0.1",
    "@angular/compiler": "~2.0.1",
    "@angular/core": "~2.0.1",
    "@angular/forms": "~2.0.1",
    "@angular/http": "~2.0.1",
    "@angular/platform-browser": "~2.0.1",
    "@angular/platform-browser-dynamic": "~2.0.1",
    "@angular/platform-server": "~2.0.1",
    "@angular/router": "~3.0.1",
    "assets-webpack-plugin": "^3.4.0",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.17",
    "ie-shim": "^0.1.0"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.2",
    "to-string-loader": "^1.1.4",
    "typescript": "2.0.3",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.8",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-version-file-plugin": "0.2.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.1",
    "html-webpack-plugin": "^2.21.0",
    "copy-webpack-plugin": "^3.0.1"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
